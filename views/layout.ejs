  <div id="main" class="documentation">
    <div id="main" class="documentation">
      <div class="wrapper">
        <div id="primary" class="full-width">
          
          <!-- the right hand menu bar. This should be a partial -->
          <div id="navmenu">
            <ul class="nav-categories">
              <% for (var catIdx in routes) { %>
                <% var category = routes[catIdx]; %>
                <% if (!category.hideinmenu && (!context || category.context === context)) { %>
                  <li><p><%= category.title %></p>
                    <ul class="nav-pages">
                      <% for (var pageIdx in category.pages) { %>
                        <% var page = category.pages[pageIdx]; %>
                        <% if ((currentPage.category === category.folder) && (currentPage.page === page.file)) { %>
                        <li class="active"><%= routes[catIdx].pages[pageIdx].title %></li>
                        <% } else { %>
                        <li><a href="<%= '/doc/' + category.folder + '/' + page.file %>">
                          <%= routes[catIdx].pages[pageIdx].title %>
                        </a></li>
                        <% } %>
                      <% } %>
                    </ul>
                  </li>
                <% }Â %>
              <% } %>
              <li><p>Support</p>
              <ul class="nav-pages">
                <li><a href="http://factory.joshfire.com/faq">FAQ</a></li>
                <li><a href="https://groups.google.com/group/joshfire-dev">Joshfire-dev Google Group</a></li>
                <li><a href="irc://irc.freenode.net/joshfire">Joshfire IRC channel</a></li>
              </ul>
            </ul>
          </div>

          <div id="layout-content">
            <!-- this will be replaced with the actual content of the page -->
            <%- body %>

            <!-- this lower simple pagination (previous, next) appears optionnaly --> 
            <div class='docNavigation'>
              <% if (locals.navPrev) { %>
                <div class='docNavigationPrev'>
                  <a href='<%= navPrev.url %>'>&larr; <%= navPrev.title %></a>
                </div>
              <% } %>
              
              <% if (locals.navNext) { %>
                <div class='docNavigationNext'>
                  <a href='<%= navNext.url %>'><%= navNext.title %> &rarr;</a>
                </div>
              <% } %>
            </div>
          </div>


        </div>
      </div>
    </div>
  </div><!-- #page -->


