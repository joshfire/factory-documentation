          <div class="menu-topmenu-container">
              <ul id="menu-topmenu" class="menu">
                <li id="menu-item-36" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-36 <%= currentPage.tab == 'home' ? 'current_page_item' : '' %>"><a href="/">Home</a></li>

                <li id="menu-item-241" class="menu-item menu-item-type-post_type menu-item-object-page current-menu-item page_item page-item-239 <%= currentPage.tab == 'doc' ? 'current_page_item' : '' %> menu-item-241"><a href="/doc">Documentation</a></li>
